{
  "name": "prosemirror-suggestion-mode",
  "version": "1.0.0",
  "description": "ProseMirror extension for suggestion/track changes mode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:memory": "node --expose-gc --max-old-space-size=4096 node_modules/.bin/jest",
    "test:debug": "jest --runInBand --no-cache --detectOpenHandles --testTimeout=10000",
    "test:unit": "jest test/unit --runInBand",
    "test:tools-minimal": "jest test/unit/tools.test.ts --no-coverage --runInBand",
    "test:tools-simple": "jest test/unit/tools-simple.test.ts --no-coverage --runInBand",
    "test:accept-reject-simple": "jest test/unit/accept-reject-simple.test.ts --no-coverage --runInBand",
    "test:setmode-simple": "jest test/unit/setMode-simple.test.ts --no-coverage --runInBand",
    "test:simple-all": "jest test/unit/*-simple.test.ts --no-coverage --runInBand",
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:lib": "tsc -p tsconfig.lib.json",
    "prepare": "npm run build:lib",
    "prepublishOnly": "npm test",
    "update-imports": "node update-imports.js"
  },
  "dependencies": {
    "prosemirror-commands": "^1.5.2",
    "prosemirror-history": "^1.4.1",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-model": "^1.19.3",
    "prosemirror-schema-basic": "^1.2.3",
    "prosemirror-schema-list": "^1.5.0",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.7.3",
    "prosemirror-view": "^1.31.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
